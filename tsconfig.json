{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "es6", "dom"],
    "sourceMap": true,
    "outDir": "./dist",    
    "allowJs": true,
    "jsx": "react",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "./src/**/*", 
    "index.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
